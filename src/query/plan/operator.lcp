(defun save-pointer (archive member-name)
  #>cpp
  SavePointer(${archive}, ${member-name});
  cpp<#)

(defun load-pointer (archive member-name)
  #>cpp
  LoadPointer(${archive}, ${member-name});
  cpp<#)

(defun save-pointers (archive member-name)
