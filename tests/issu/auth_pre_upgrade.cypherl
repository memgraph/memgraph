CREATE DATABASE db1;
CREATE ROLE system_admin;
GRANT ALL PRIVILEGES TO system_admin;
GRANT DATABASE memgraph TO system_admin;
GRANT CREATE ON NODES CONTAINING LABELS *, READ ON NODES CONTAINING LABELS *, UPDATE ON NODES CONTAINING LABELS *, DELETE ON NODES CONTAINING LABELS * TO system_admin;
GRANT CREATE ON EDGES CONTAINING TYPES *, READ ON EDGES CONTAINING TYPES *, UPDATE ON EDGES CONTAINING TYPES *, DELETE ON EDGES CONTAINING TYPES * TO system_admin;

CREATE ROLE tenant1_admin;
CREATE ROLE tenant1_user;

GRANT ALL PRIVILEGES TO tenant1_admin;
GRANT CREATE ON NODES CONTAINING LABELS *, READ ON NODES CONTAINING LABELS *, UPDATE ON NODES CONTAINING LABELS *, DELETE ON NODES CONTAINING LABELS * TO tenant1_admin;
GRANT CREATE ON EDGES CONTAINING TYPES *, READ ON EDGES CONTAINING TYPES *, UPDATE ON EDGES CONTAINING TYPES *, DELETE ON EDGES CONTAINING TYPES * TO tenant1_admin;
GRANT MATCH, CREATE, MERGE, SET, DELETE TO tenant1_user;

GRANT DATABASE db1 TO tenant1_admin;
GRANT DATABASE db1 TO tenant1_user;

CREATE USER system_admin_user IDENTIFIED BY 'admin_password';
CREATE USER tenant1_admin_user IDENTIFIED BY 't1_admin_pass';
CREATE USER tenant1_regular_user IDENTIFIED BY 't1_user_pass';

SET ROLE FOR system_admin_user TO system_admin;
SET ROLE FOR tenant1_admin_user TO tenant1_admin;
SET ROLE FOR tenant1_regular_user TO tenant1_user;
