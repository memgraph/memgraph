{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "Attach to PID",
            "target": "55243",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal",
            "args": [
                "--workloads-root-directory",
                "${workspaceFolder}/tests/e2e"
            ]
        },
        {
            "name": "(lldb) Launch",
            "type": "lldb",
            "request": "launch",
            // Resolved by CMake Tools:
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                // For running a specific unit test
                "--gtest_filter=InterpreterTest.AllowLoadCsvConfig"
                // For e2e tests
                // "--bolt-port",
                // "7687"
            ],
            "stopAtEntry": false,
            "cwd": "${command:cmake.launchTargetDirectory}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(lldb) Launch Memgraph",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/memgraph",
            "args": [
                "--also-log-to-stderr",
                "--log-level=TRACE",
                "--telemetry-enabled=False",
                "--query-execution-timeout-sec=0",
                //"--query-modules-directory=/home/josipmrden/Memgraph-Repos/cpp_dummy_module/build/src",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "GDB Memgraph",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/memgraph",
            "args": [
                "--telemetry-enabled=False",
                "--also-log-to-stderr",
                "--storage-properties-on-edges=True",
                "--log-level=TRACE",
                "--storage-recover-on-startup=true",
                "--storage-snapshot-on-exit=true",
               //"--query-modules-directory=/home/josipmrden/Memgraph-Repos/cpp_dummy_module/build/src",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
